		{
			"name": "list_of_books(fiction)",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"const response = pm.response.json();\r",
							"\r",
							"// filter to get the first avilable book\r",
							"const fiction_books = response.filter((book) => book.available == true);\r",
							"\r",
							"const book = fiction_books[0];\r",
							"\r",
							"if (book) {\r",
							"    pm.globals.set(\"book_id\", book.id);\r",
							"}\r",
							"\r",
							"pm.test(\"Book verification\", () => {\r",
							"    pm.expect(book).to.be.an('object');\r",
							"    pm.expect(book.available).to.be.true;\r",
							"    pm.expect(book.available).to.eql(true);\r",
							"    pm.expect(book.type).to.eql('fiction');\r",
							"});\r",
							"\r",
							"pm.globals.set(\"book_id\", book.id);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
